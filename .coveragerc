[run]
source = .
omit =
    */migrations/*
    */venv/*
    */env/*
    */__pycache__/*
    */tests/*
    */test_*.py
    manage.py
    */settings/*
    */wsgi.py
    */asgi.py
    */urls.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov

